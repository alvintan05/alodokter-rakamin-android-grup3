<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.grup3.alodokter_rakamin_android_grup3">

    <application
        android:name=".di.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_alodokter_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_alodokter_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Alodokterrakaminandroidgrup3">
        <activity
            android:name=".ui.ChangePasswordActivity"
            android:exported="true" />
        <activity
            android:name=".ui.DetailArticleActivity"
            android:exported="true"/>
        <activity
            android:name=".ui.SignUpActivity"
            android:exported="true" />
        <activity
            android:name=".ui.SignInActivity"
            android:exported="true"/>
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>